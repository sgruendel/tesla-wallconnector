{
  "name": "tesla-wallconnector",
  "version": "1.0.0",
  "description": "Logging Tesla Wall Connector data with mongodb",
  "main": "index.js",
  "scripts": {
    "reinstall": "rm -rf ./node_modules && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgruendel/tesla-wallconnector.git"
  },
  "keywords": [
    "tesla",
    "wallconnector",
    "mongodb"
  ],
  "author": "Stefan Gr√ºndel",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sgruendel/tesla-wallconnector/issues"
  },
  "homepage": "https://github.com/sgruendel/tesla-wallconnector#readme",
  "dependencies": {
    "mongoose": "^6.11.0",
    "node-fetch": "^2.6.11",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "eslint": "^8.9.0",
    "eslint-config-strongloop": "^2.1.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "extends": "strongloop",
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "max-len": [
        "error",
        120,
        4,
        {
          "ignoreComments": true,
          "ignoreUrls": true,
          "ignoreStrings": true,
          "ignorePattern": "^\\s*var\\s.+=\\s*(require\\s*\\()|(/)"
        }
      ]
    }
  }
}